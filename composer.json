{
    "name": "gupalo/symfony-form-transformers",
    "description": "Symfony Form Transformers",
    "keywords": ["tsv", "symfony", "form", "data-transformer"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Volodymyr Luchaninov",
            "homepage": "https://luchaninov.com"
        }
    ],
    "require": {
        "php": ">=8.4",
        "symfony/form": "^7.4|^8.0",
        "symfony/yaml": "^7.4|^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.4"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gupalo\\SymfonyFormTransformers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\SymfonyFormTransformers\\Tests\\": "tests/"
        }
    }
}
